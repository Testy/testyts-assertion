{
    "name": "@testy/assertion",
    "version": "0.1.2",
    "description": "Testy.Ts assertion library",
    "main": "build/core.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Testy/testyts-assertion.git"
    },
    "scripts": {
        "build": "tsc",
        "check-coverage": "nyc --reporter=lcov --reporter=html npm run test && nyc report",
        "test": "testyts"
    },
    "keywords": [
        "assertion",
        "test",
        "testyts",
        "typescript"
    ],
    "git": "https://github.com/Testy/testyts-assertion.git",
    "author": "Antoine Boisier-Michaud",
    "license": "ISC",
    "devDependencies": {
        "codecov": "^3.3.0",
        "nyc": "^13.3.0",
        "testyts": "0.7.1"
    },
    "nyc": {
        "include": [
            "src/lib/**/*.ts"
        ],
        "exclude": [
            "src/spec/**/*.ts"
        ],
        "extension": [
            ".ts"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "sourceMap": true,
        "instrument": true
    }
}
